#include <stdio.h>
#include <string.h>

void win() {
    printf("Congratulations! Here's your flag: CTF{b1n4ry_n1nj4_l3v3l_1}\n");
}

int main() {
    char buffer[64];
    printf("Enter the secret code: ");
    gets(buffer);
    
    if(strcmp(buffer, "secret_code") == 0) {
        printf("Correct! But can you find another way in?\n");
    } else {
        printf("Wrong code!\n");
    }
    return 0;
}
